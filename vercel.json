{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.js" },
    { "src": "/docs/(.*)", "dest": "/view/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
