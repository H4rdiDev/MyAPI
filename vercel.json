{
  "version": 2,
  "builds": [
    {
      "src": "index.js", // Sesuaikan dengan file utama aplikasi Express.js Anda
      "use": "@vercel/node" // Gunakan environment node untuk menjalankan aplikasi
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js" // Routing semua request ke index.js (atau file utama Anda)
    }
  ]
}
